{
    "format_version": "1.18.3",
    "animation_controllers": {
        "controller.animation.containment_chamber": {
            "initial_state": "water_fill",
            "states": {
                "water_fill": {
                    "animations": [
                        "water_fill"
                    ],
                    "transitions": [
                        {
                            "breaking": "q.property('pokeb:state') == 'breaking'"
                        },
                        {
                            "opening": "q.property('pokeb:state') == 'opening'"
                        },
                        {
                            "opening_ditto": "q.property('pokeb:state') == 'opening_ditto'"
                        },
                        {
                            "closing": "q.property('pokeb:state') == 'closing'"
                        }
                    ]
                },
                "breaking": {
                    "animations": [
                        "breaking"
                    ],
                    "transitions": [
                        {
                            "water_fill": "q.property('pokeb:state') == 'water_fill'"
                        },
                        {
                            "opening": "q.property('pokeb:state') == 'opening'"
                        },
                        {
                            "opening_ditto": "q.property('pokeb:state') == 'opening_ditto'"
                        },
                        {
                            "closing": "q.property('pokeb:state') == 'closing'"
                        }
                    ]
                },
                "opening": {
                    "animations": [
                        "opening"
                    ],
                    "transitions": [
                        {
                            "water_fill": "q.property('pokeb:state') == 'water_fill'"
                        },
                        {
                            "breaking": "q.property('pokeb:state') == 'breaking'"
                        },
                        {
                            "opening_ditto": "q.property('pokeb:state') == 'opening_ditto'"
                        },
                        {
                            "closing": "q.property('pokeb:state') == 'closing'"
                        }
                    ]
                },
                "opening_ditto": {
                    "animations": [
                        "opening_ditto"
                    ],
                    "transitions": [
                        {
                            "water_fill": "q.property('pokeb:state') == 'water_fill'"
                        },
                        {
                            "breaking": "q.property('pokeb:state') == 'breaking'"
                        },
                        {
                            "opening": "q.property('pokeb:state') == 'opening'"
                        },
                        {
                            "closing": "q.property('pokeb:state') == 'closing'"
                        }
                    ]
                },
                "closing": {
                    "animations": [
                        "closing"
                    ],
                    "transitions": [
                        {
                            "water_fill": "q.property('pokeb:state') == 'water_fill'"
                        },
                        {
                            "breaking": "q.property('pokeb:state') == 'breaking'"
                        },
                        {
                            "opening": "q.property('pokeb:state') == 'opening'"
                        },
                        {
                            "opening_ditto": "q.property('pokeb:state') == 'opening_ditto'"
                        }
                    ]
                }
            }
        }
    }
}